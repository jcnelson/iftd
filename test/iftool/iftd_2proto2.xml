<iftd>

   <filechunks path="/tmp/iftd-experiments/files" />
   <send_files path="/tmp/" />
   <recv_files path="/tmp/" />

   <xmlrpc port="4002" dir="RPC2"/>
   <stats retrain_freq="25" classifier="NaiveBayes" />

   <protocol name="http">
      <sender>
         <setup type="int"  PROTO_PORTNUM="8001"/>
      </sender>
      <receiver>
         <setup type="int" PROTO_PORTNUM="8000"/>
      </receiver>
   </protocol>

   <protocol name="iftsocket">
      <sender>
         <setup type="int" PROTO_PORTNUM="8081"/>
      </sender>
      <receiver>
         <setup type="int" PROTO_PORTNUM="8082"/>
      </receiver>
   </protocol>
   
</iftd>
